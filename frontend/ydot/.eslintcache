         }}>\n                    {leng}<div style={{ display: \"inline-block\", fontWeight: \"normal\", textDecorationLine: \"underline\", fontSize: 12 }}>개</div>\n                </div>\n            </div>\n        </>\n    )\n}\n\n//SplashScreen.js 요소\nexport function GuideBox({ navigation, icon, title, content, button, center }) {\n    return (\n        <>\n            <div style={{\n                width: 400,\n                backgroundColor: \"#ffffff\",\n                borderRadius: 30,\n                paddingTop: 30,\n                paddingBottom: 30,\n                display: \"flex\",\n                flexDirection: \"column\",\n                alignItems: \"center\",\n                marginLeft: 20,\n                marginRight: 20,\n            }}>\n                {icon}\n                <p style={{\n                    display: \"inline-block\",\n                    fontSize: 18,\n                    color: \"#202426\",\n                    fontWeight: \"bold\",\n                    marginTop: 20,\n                    marginBottom: 0,\n                }}>{title}</p>\n                <p style={{\n                    display: \"inline-block\",\n                    fontSize: 16,\n                    opacity: 0.8,\n                    color: \"#202426\",\n                    height: 116,\n                    lineHeight: 1.88\n                }}>{content}</p>\n                <button style={{\n                    cursor: \"pointer\",\n                    outline: 0,\n                    display: \"flex\",\n                    flexDirection: \"row\",\n                    width: 200,\n                    height: 48,\n                    marginTop: 20,\n                    backgroundColor: \"#ffffff\",\n                    borderWidth: 2,\n                    borderColor: \"#202426\",\n                    borderRadius: 7,\n                    alignSelf: \"center\",\n                    alignItems: \"center\",\n                    justifyContent: \"center\"